{
  "name": "esri-map",
  "version": "0.0.0",
  "scripts": {
    "start": "ng serve",
    "start:prod": "ng serve --prod",
    "build": "ng build",
    "build:prod": "ng build --prod",
    "lint": "ng lint",
    "docs": "npm run docs:build && npm run docs:cp",
    "docs:build": "ng build --prod --output-path docs --base-href esri-map",
    "docs:cp": "cp docs/index.html docs/404.html && cp LICENSE docs/LICENSE.txt",
    "build-lib": "ng build ng-esri-map && cp CHANGELOG.md dist/ng-esri-map",
    "npm-pack": "cd dist/ng-esri-map && npm pack",
    "package": "npm run build-lib && npm run npm-pack",
    "update:dependencies": "node update-dependencies.js"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "8.2.4",
    "@angular/compiler": "8.2.4",
    "@angular/core": "8.2.4",
    "@angular/forms": "8.2.4",
    "@angular/platform-browser": "8.2.4",
    "@angular/platform-browser-dynamic": "8.2.4",
    "@types/arcgis-js-api": "4.12.0",
    "esri-loader": "2.10.0",
    "rxjs": "6.4.0",
    "zone.js": "0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.803.2",
    "@angular-devkit/build-ng-packagr": "0.803.2",
    "@angular/cli": "8.3.2",
    "@angular/compiler-cli": "8.2.4",
    "@angular/language-service": "8.2.4",
    "codelyzer": "5.1.0",
    "ng-packagr": "5.5.0",
    "tsickle": "0.37.0",
    "tslib": "1.10.0",
    "tslint": "5.19.0",
    "typescript": "3.5.3"
  }
}
